TotalKills.Text = "Total Kills: " .. totalKills.Value
totalKills.Changed:Connect(function()
TotalKills.Text = "Total Kills: " .. totalKills.Value
end)
KillsGranted.Text = "Total Monthly Kills: " .. monthlyKills.Value
monthlyKills.Changed:Connect(function()
KillsGranted.Text = "Total Monthly Kills: " .. monthlyKills.Value
end)
task.wait(.1)
while wait(.1) do
    if player.Backpack:FindFirstChild('Shove') then
        humanoidRootPart.CFrame = part.CFrame * CFrame.new(0,11,0)
        for _, tool in pairs(player.Backpack:GetChildren()) do
            if tool:IsA('Tool') then
                humanoid:EquipTool(tool)
                task.wait(.001)
                humanoid:UnequipTools()
                mouse1click()
                mouse1release()
                task.wait(.001)
            end
        end
    else
        Lxchu.Enabled = false
        humanoidRootPart.CFrame = part.CFrame * CFrame.new(0,11,-2)
        player.CharacterAdded:Connect(function(char)
            humanoidRootPart = char:WaitForChild('HumanoidRootPart')
            humanoidRootPart.CFrame = part.CFrame * CFrame.new(0,11,-2)
        end)
    end
end
